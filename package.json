{
  "name": "vitevueapp",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build",
    "build:check": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:no-cache": "yarn clean:cache && npm run build",
    "build:ssr": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.ts",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vicons/antd": "^0.11.0",
    "@vueuse/core": "^8.6.0",
    "ant-design-vue": "^3.2.7",
    "axios": "^0.24.0",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "echarts": "^5.2.1",
    "element-resize-detector": "^1.2.4",
    "express": "^4.18.1",
    "fs-extra": "^10.0.0",
    "ip": "^1.1.8",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "makeit-captcha": "^1.2.5",
    "pinia": "^2.0.0-rc.4",
    "serialize-javascript": "^6.0.0",
    "vue": "^3.2.16",
    "vue-router": "^4.0.11",
    "vue-types": "^4.1.0",
    "vuedraggable": "^2.24.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "naive-ui": "^2.19.11",
    "typescript": "^4.3.2",
    "vfonts": "^0.1.0",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
  }
}
